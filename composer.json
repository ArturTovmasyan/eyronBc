{
    "name": "aram/bucketlist",
    "license": "proprietary",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "2.5.4",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",

        "friendsofsymfony/user-bundle": "~1.3",
        "hwi/oauth-bundle": "0.3.*",
        "nelmio/api-doc-bundle": "2.11.*",
        "friendsofsymfony/rest-bundle": "1.7.*",

        "sonata-project/admin-bundle": "2.3.*",
        "sonata-project/core-bundle": "2.3.10",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*",
        "sonata-project/intl-bundle": "2.2.*",
        "sonata-project/easy-extends-bundle" :"2.1.7",

        "stof/doctrine-extensions-bundle": "1.2.*",

        "jms/serializer-bundle": "~0.12",
        "jms/security-extra-bundle": "1.6.*",

        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/doctrine-migrations-bundle": "1.0.*",
        "oro/doctrine-extensions": "1.0.*",
        "friendsofsymfony/comment-bundle": "2.0.*",
        "knplabs/knp-paginator-bundle": "2.4.*",
        "stfalcon/tinymce-bundle": "^0.4.0",
        "a2lix/translation-form-bundle": "1.*",
        "gedmo/doctrine-extensions": "^2.4",
        "liip/imagine-bundle" : "1.3.*",
        "presta/sitemap-bundle": "^1.4",
        "richsage/rms-push-notifications-bundle": "0.1.*",
        "knplabs/knp-markdown-bundle": "~1.3",
        "suncat/mobile-detect-bundle": "^0.10.3",
        "friendsofsymfony/http-cache-bundle": "~1.0",
        "genemu/form-bundle": "2.3.0",
        "mandrill/mandrill": "1.0.*"
    },
    "require-dev": {
        "behat/behat": "3.0.*",
        "behat/mink-selenium2-driver": "1.2.*",
        "behat/symfony2-extension": "2.0.*",
        "behat/mink-browserkit-driver": "1.2.*",
        "behat/mink-extension": "~2.0",
        "behat/mink-goutte-driver": "~1.1",
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.3.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
