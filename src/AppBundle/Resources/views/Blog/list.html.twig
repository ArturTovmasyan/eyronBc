{% extends "::unp.html.twig" %}

{% block content %}
    <div class="container">
        <div  class="signup">
            <div class="row">
                {% for blog in blogs %}
                    <h1 class="text-center"><a href="{{ path('blog_show', {'slug': blog.slug}) }}">{{ blog.title }}</a></h1>
                {% endfor %}
            </div>
            <div class="text-center">
                {{ knp_pagination_render(blogs) }}
            </div>
        </div>
    </div>
{% endblock %}

