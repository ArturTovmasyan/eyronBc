{% extends 'SonataAdminBundle:CRUD:base_show_field.html.twig' %}

{% block field %}
    {% for k,videoLink in value %}
        <youtube-video
           player-width="'420px'"
           player-height="'315px'"
           data-ng-init="adminShowVideos[{{ k }}]='{{ videoLink }}'"
           video-url="adminShowVideos[{{ k }}]">
        </youtube-video>
    {% endfor %}
{% endblock %}